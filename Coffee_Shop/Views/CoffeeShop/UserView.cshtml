@{
    ViewData["Title"] = "User Information";
}

@model Coffee_Shop.Models.RegisterUser

<h1>Your Information</h1>
@{

<table class="table">
    <tr>
        <th>UserName</th>
        <th>Email</th>
        <th>Password</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
    </tr>
    <tr>
        <td>@Model.UserName</td>
        <td>@Model.Email</td>
        <td>@Model.Password</td>
        <td>@Model.FirstName</td>
        <td>@Model.LastName</td>
        <td>@Model.Age</td>
    </tr>

</table>

}

